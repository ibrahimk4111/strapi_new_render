{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "prod_name": {
      "type": "string"
    },
    "img": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "rating": {
      "type": "decimal"
    },
    "description": {
      "type": "richtext"
    },
    "inStock": {
      "type": "boolean",
      "default": true
    },
    "pack_size": {
      "type": "string"
    },
    "categories": {
      "type": "enumeration",
      "enum": [
        "medicine",
        "oil"
      ]
    }
  }
}
